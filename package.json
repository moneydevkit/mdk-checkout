{
  "name": "mdk-checkout",
  "version": "0.0.66",
  "description": "Money Dev Kit checkout components and server helpers for Next.js.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./mdk-styles.css": "./dist/mdk-styles.css",
    "./package.json": "./package.json",
    "./server/runtime": {
      "types": "./dist/server/runtime.d.ts",
      "import": "./dist/server/runtime.js",
      "default": "./dist/server/runtime.js"
    },
    "./server/webhooks": {
      "types": "./dist/server/webhooks.d.ts",
      "import": "./dist/server/webhooks.js",
      "default": "./dist/server/webhooks.js"
    },
    "./server/pay_bolt_12": {
      "types": "./dist/server/pay_bolt_12.d.ts",
      "import": "./dist/server/pay_bolt_12.js",
      "default": "./dist/server/pay_bolt_12.js"
    },
    "./next-plugin": {
      "types": "./dist/next-plugin.d.ts",
      "import": "./dist/next-plugin.js",
      "default": "./dist/next-plugin.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && postcss src/mdk-styles.css -o dist/mdk-styles.css",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "pack:local": "npm pack"
  },
  "peerDependencies": {
    "next": "^14 || ^15",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@moneydevkit/api-contract": "^0.1.10",
    "@moneydevkit/lightning-js": "^0.1.11",
    "@orpc/client": "1.3.0",
    "@orpc/contract": "1.3.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.79.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "qrcode.react": "^4.2.0",
    "react-hook-form": "^7.56.4",
    "tailwind-merge": "^3.3.0",
    "undici": "^7.16.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20.10.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.13",
