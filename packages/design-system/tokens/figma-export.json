{
  "$schema": "https://design-tokens.org/schema.json",
  "$description": "MoneyDevKit Design Tokens - exported from Figma",
  "color": {
    "$description": "Color tokens",
    "brand": {
      "primary": {
        "$type": "color",
        "$value": "#00E599",
        "$description": "Primary brand color (green)"
      },
      "primary-hover": {
        "$type": "color",
        "$value": "#00CC88",
        "$description": "Primary brand color on hover"
      }
    },
    "neutral": {
      "50": { "$type": "color", "$value": "#fafafa" },
      "100": { "$type": "color", "$value": "#f4f4f5" },
      "200": { "$type": "color", "$value": "#e4e4e7" },
      "300": { "$type": "color", "$value": "#d4d4d8" },
      "400": { "$type": "color", "$value": "#a1a1aa" },
      "500": { "$type": "color", "$value": "#71717a" },
      "600": { "$type": "color", "$value": "#52525b" },
      "700": { "$type": "color", "$value": "#3f3f46" },
      "800": { "$type": "color", "$value": "#27272a" },
      "900": { "$type": "color", "$value": "#18181b" },
      "950": { "$type": "color", "$value": "#09090b" }
    },
    "semantic": {
      "light": {
        "background": { "$type": "color", "$value": "oklch(0.99 0 0)" },
        "foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "card": { "$type": "color", "$value": "oklch(1 0 0)" },
        "card-foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "popover": { "$type": "color", "$value": "oklch(0.99 0 0)" },
        "popover-foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "primary": { "$type": "color", "$value": "oklch(0 0 0)" },
        "primary-foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "secondary": { "$type": "color", "$value": "oklch(0.94 0 0)" },
        "secondary-foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "muted": { "$type": "color", "$value": "oklch(0.97 0 0)" },
        "muted-foreground": { "$type": "color", "$value": "oklch(0.44 0 0)" },
        "accent": { "$type": "color", "$value": "oklch(0.94 0 0)" },
        "accent-foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "destructive": { "$type": "color", "$value": "oklch(0.63 0.19 23.03)" },
        "destructive-foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "border": { "$type": "color", "$value": "oklch(0.92 0 0)" },
        "input": { "$type": "color", "$value": "oklch(0.94 0 0)" },
        "ring": { "$type": "color", "$value": "oklch(0 0 0)" }
      },
      "dark": {
        "background": { "$type": "color", "$value": "oklch(0 0 0)" },
        "foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "card": { "$type": "color", "$value": "oklch(0.14 0 0)" },
        "card-foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "popover": { "$type": "color", "$value": "oklch(0.18 0 0)" },
        "popover-foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "primary": { "$type": "color", "$value": "oklch(1 0 0)" },
        "primary-foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "secondary": { "$type": "color", "$value": "oklch(0.25 0 0)" },
        "secondary-foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "muted": { "$type": "color", "$value": "oklch(0.23 0 0)" },
        "muted-foreground": { "$type": "color", "$value": "oklch(0.72 0 0)" },
        "accent": { "$type": "color", "$value": "oklch(0.32 0 0)" },
        "accent-foreground": { "$type": "color", "$value": "oklch(1 0 0)" },
        "destructive": { "$type": "color", "$value": "oklch(0.69 0.20 23.91)" },
        "destructive-foreground": { "$type": "color", "$value": "oklch(0 0 0)" },
        "border": { "$type": "color", "$value": "oklch(0.26 0 0)" },
        "input": { "$type": "color", "$value": "oklch(0.32 0 0)" },
        "ring": { "$type": "color", "$value": "oklch(0.72 0 0)" }
      }
    },
    "chart": {
      "1": { "$type": "color", "$value": "oklch(0.81 0.17 75.35)" },
      "2": { "$type": "color", "$value": "oklch(0.55 0.22 264.53)" },
      "3": { "$type": "color", "$value": "oklch(0.72 0 0)" },
      "4": { "$type": "color", "$value": "oklch(0.92 0 0)" },
      "5": { "$type": "color", "$value": "oklch(0.56 0 0)" }
    }
  },
  "spacing": {
    "$description": "Spacing scale",
    "0": { "$type": "dimension", "$value": "0px" },
    "1": { "$type": "dimension", "$value": "0.25rem" },
    "2": { "$type": "dimension", "$value": "0.5rem" },
    "3": { "$type": "dimension", "$value": "0.75rem" },
    "4": { "$type": "dimension", "$value": "1rem" },
    "5": { "$type": "dimension", "$value": "1.25rem" },
    "6": { "$type": "dimension", "$value": "1.5rem" },
    "8": { "$type": "dimension", "$value": "2rem" },
    "10": { "$type": "dimension", "$value": "2.5rem" },
    "12": { "$type": "dimension", "$value": "3rem" },
    "16": { "$type": "dimension", "$value": "4rem" },
    "20": { "$type": "dimension", "$value": "5rem" },
    "24": { "$type": "dimension", "$value": "6rem" }
  },
  "radius": {
    "$description": "Border radius tokens",
    "none": { "$type": "dimension", "$value": "0" },
    "sm": { "$type": "dimension", "$value": "0.125rem" },
    "default": { "$type": "dimension", "$value": "0.5rem" },
    "md": { "$type": "dimension", "$value": "0.375rem" },
    "lg": { "$type": "dimension", "$value": "0.5rem" },
    "xl": { "$type": "dimension", "$value": "0.75rem" },
    "2xl": { "$type": "dimension", "$value": "1rem" },
    "full": { "$type": "dimension", "$value": "9999px" }
  },
  "shadow": {
    "$description": "Box shadow tokens",
    "2xs": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.09)" },
    "xs": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.09)" },
    "sm": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 1px 2px -1px hsl(0 0% 0% / 0.18)" },
    "default": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 1px 2px -1px hsl(0 0% 0% / 0.18)" },
    "md": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 2px 4px -1px hsl(0 0% 0% / 0.18)" },
    "lg": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 4px 6px -1px hsl(0 0% 0% / 0.18)" },
    "xl": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.18), 0px 8px 10px -1px hsl(0 0% 0% / 0.18)" },
    "2xl": { "$type": "shadow", "$value": "0px 1px 2px 0px hsl(0 0% 0% / 0.45)" }
  },
  "typography": {
    "$description": "Typography tokens",
    "fontFamily": {
      "sans": { "$type": "fontFamily", "$value": "Geist, ui-sans-serif, system-ui, sans-serif" },
      "serif": { "$type": "fontFamily", "$value": "Georgia, ui-serif, serif" },
      "mono": { "$type": "fontFamily", "$value": "Geist Mono, ui-monospace, monospace" }
    },
    "fontSize": {
      "xs": { "$type": "dimension", "$value": "0.75rem" },
      "sm": { "$type": "dimension", "$value": "0.875rem" },
      "base": { "$type": "dimension", "$value": "1rem" },
      "lg": { "$type": "dimension", "$value": "1.125rem" },
      "xl": { "$type": "dimension", "$value": "1.25rem" },
      "2xl": { "$type": "dimension", "$value": "1.5rem" },
      "3xl": { "$type": "dimension", "$value": "1.875rem" },
      "4xl": { "$type": "dimension", "$value": "2.25rem" }
    },
    "fontWeight": {
      "normal": { "$type": "fontWeight", "$value": "400" },
      "medium": { "$type": "fontWeight", "$value": "500" },
      "semibold": { "$type": "fontWeight", "$value": "600" },
      "bold": { "$type": "fontWeight", "$value": "700" }
    },
    "lineHeight": {
      "tight": { "$type": "number", "$value": "1.25" },
      "normal": { "$type": "number", "$value": "1.5" },
      "relaxed": { "$type": "number", "$value": "1.75" }
    }
  }
}
