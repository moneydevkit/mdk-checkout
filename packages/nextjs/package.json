{
  "name": "@moneydevkit/nextjs",
  "version": "0.6.0-beta.5",
  "title": "@moneydevkit/nextjs",
  "description": "moneydevkit checkout components for Next.js.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moneydevkit/mdk-checkout.git",
    "directory": "packages/nextjs"
  },
  "homepage": "https://github.com/moneydevkit/mdk-checkout/blob/main/packages/nextjs/README.md",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./mdk-styles.css": "./dist/mdk-styles.css",
    "./package.json": "./package.json",
    "./server/route": {
      "types": "./dist/server/route.d.ts",
      "import": "./dist/server/route.js",
      "default": "./dist/server/route.js"
    },
    "./next-plugin": {
      "types": "./dist/next-plugin.d.ts",
      "import": "./dist/next-plugin.js",
      "default": "./dist/next-plugin.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm --prefix ../core run build",
    "build": "rm -rf dist && tsc -p tsconfig.json && npm run copy-css",
    "copy-css": "node ../core/scripts/copy-styles.mjs dist/mdk-styles.css",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "npm run build && node --test --experimental-loader ./tests/test-loader.mjs tests/route.test.js",
    "pack:local": "npm pack"
  },
  "peerDependencies": {
    "next": "^15 || ^16",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@moneydevkit/api-contract": "^0.1.13",
    "@moneydevkit/core": "0.6.0-beta.5",
    "@moneydevkit/lightning-js": "^0.1.57",
    "@orpc/client": "1.3.0",
    "@orpc/contract": "1.3.0",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.79.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "qrcode.react": "^4.2.0",
    "react-hook-form": "^7.56.4",
    "tailwind-merge": "^3.3.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.6.3"
  },
  "module": "./dist/index.js"
}