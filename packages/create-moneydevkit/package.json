{
  "name": "create-moneydevkit",
  "version": "0.1.1-beta.0",
  "description": "Interactive CLI for bootstrapping Money Dev Kit credentials.",
  "type": "module",
  "bin": {
    "create-moneydevkit": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "run:local": "npm run build && node dist/index.js --base-url http://localhost:3900",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@moneydevkit/api-contract": "^0.1.11",
    "@moneydevkit/lightning-js": "^0.1.46",
    "@orpc/client": "^1.3.0",
    "@orpc/contract": "^1.3.0",
    "bip39": "^3.1.0",
    "clipboardy": "^4.0.0",
    "minimist": "^1.2.8",
    "open": "^10.1.0",
    "set-cookie-parser": "^2.6.0",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.9.0",
    "@types/set-cookie-parser": "^2.4.10",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.15.0",
    "tsup": "^8.4.0",
    "typescript": "^5.6.3",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.15.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moneydevkit/mdk-checkout.git",
    "directory": "packages/create-moneydevkit"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "moneydevkit",
    "lightning",
    "cli"
  ]
}
